{
  "name": "mediprep",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "migrate-create": "node-pg-migrate -m ./stack/migrations create",
    "migrate-up": "node-pg-migrate -m ./stack/migrations up",
    "migrate-down": "node-pg-migrate -m ./stack/migrations down",
    "migrate-redo": "node-pg-migrate -m ./stack/migrations redo",
    "drizzle-generate": "drizzle-kit --config=stack/drizzle/drizzle.config.ts generate",
    "drizzle-migrate": "drizzle-kit --config=stack/drizzle/drizzle.config.ts migrate",

    "dev": "ts-node-dev --respawn --rs --files src/index.ts",
    "build": "tsc -p .",
    "drizzle-seed": "npm run build && node dist/stack/drizzle/seeds",
    "start": "node dist/src",
    "pm2": "tsc && pm2 start pm2.config.js",
    "pm2-stop": "pm2 stop all"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/communication-email": "^1.0.0",
    "@azure/identity": "^4.2.1",
    "@azure/storage-blob": "^12.23.0",
    "@paystack/paystack-sdk": "^1.0.1",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.16",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.11",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.2",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "npm": "^10.8.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "paystack-sdk": "^2.5.17",
    "pg": "^8.12.0",
    "pg-error-enum": "^0.7.1",
    "pm2": "^5.4.2",
    "redis": "^4.7.0",
    "stripe": "^16.8.0",
